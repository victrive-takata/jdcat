<?xml version="1.0" encoding="UTF-8" ?>
<schema>
	<h1>Alternative title</h1>

	<category name="item">
		<border>stdyDscr->citation->titlStmt->altTitl</border>
		<label>O (Optional)</label>
		<repeat>0-N (Repeatable)</repeat>

		<attribute>
			<border>xml:lang</border>
			<label>MA (Mandatory when applicable)</label>
			<repeat>0-1 (Non-repeatable)</repeat>
		</attribute>
	</category>

	<category name="description">
		<list>
			<li>Enter an alternative title for the survey data, such as an alias or abbreviation.</li>
		</list>
	</category>

	<category name="mapping">
		<list>
			<li>dcterms:alternative</li>
		</list>
	</category>
</schema>